Cell[CellGroupData[{

Cell["Problem 10", "Section",
 CellChangeTimes->{{3.5931515201300917`*^9, 3.593151522202105*^9}}],

Cell[TextData[{
 "We want to maximize ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"|", 
     SubsuperscriptBox["w", "ij", 
      RowBox[{"(", "l", ")"}]], "|"}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"l", "=", "1"}], "L"], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["d", 
         RowBox[{"(", 
          RowBox[{"l", "-", "1"}], ")"}]], "+", "1"}], ")"}], 
      SuperscriptBox["d", 
       RowBox[{"(", "l", ")"}]]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " subject to the conditions ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["d", 
     RowBox[{"(", "i", ")"}]], "\[GreaterEqual]", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["d", 
     RowBox[{"(", "0", ")"}]], "=", "9"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["d", "L"], "=", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"(", 
