Cell[TextData[{
 "The answer is ",
 StyleBox["D",
  FontWeight->"Bold"],
 ". Calculating ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["x", "i", 
    RowBox[{"(", "1", ")"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " for ",
